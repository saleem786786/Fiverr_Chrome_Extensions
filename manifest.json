{
  "name": "Fiverr Alert System",
  "description": "Fiverr Alert System",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
      "activeTab",
      "tabs",
      "notifications",
      "http://*/*",
      "https://*/*"
  ],

  "browser_action": {
    "default_icon": "micon.ico"
  }
}
